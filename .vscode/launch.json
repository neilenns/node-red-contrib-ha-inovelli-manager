{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch NodeRed - Linux",
      "skipFiles": ["<node_internals>/**"],
      "program": "/usr/local/share/npm-global/lib/node_modules/node-red/red.js",
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
